type Player {
    id: ID!
    name: String
    team: Team
}

enum Team {
    CSK
    MI
    DC
    GT
}

type Query {
    findAll: [Player]
    findOne(id: ID): Player
}

type Mutation {
    createPlayer(name: String, team: Team): Player
    updatePlayer(id: ID, name: String, team: Team): Player
    deletePlayer(id: ID): Player
}
